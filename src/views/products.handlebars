<div>
    <h2>products</h2>
    {{#each products}}
         <li>{{this.title}} - {{this.description}} - {{this.price}} - {{this.code}} - {{this.stock}}</li>

    {{/each}}
    {{#if hasPrevPage}}
            <a href="products?pag={{prevPage}}">Anterior</a>
        {{/if}}
        <label>{{page}}</label>
        {{#if hasNextPage}}
            <a href="/products?pag={{nextPage}}">Siguiente</a>
        {{/if}}
</div>